<form action="[% links.base_uri %]search-results"
     enctype="application/x-www-form-urlencoded"
      method="get" name="search-file">
   <input  class="pathname" name="query"
            type="textfield" value="[% prefs.query %]">
   <button class="btn btn-submit" type="submit">[% loc( 'Search' ) %]</button>
</form>
