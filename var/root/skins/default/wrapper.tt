<!doctype html>
<html lang="en">
<head>
   <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
   <meta name="description" content="[% config.description %]">
   <meta name="keywords" content="[% config.keywords %]">
   <meta name="author" content="[% config.author %]">
   <!-- Mobile -->
   <meta name="apple-mobile-web-app-capable" content="yes">
   <meta name="viewport" content="width=device-width, initial-scale=1.0">
[% IF config.colours.0 -%]
   <link href="[% uri_for( config.images ) %]favicon.png"
          rel="icon" type="image/png" >
[% ELSE -%]
   <link href="[% uri_for( config.images ) %]favicon-[% prefs.theme %].png"
          rel="icon" type="image/png">
[% END -%]
[% IF config.font -%]
   <!-- Font -->
   <link href="[% config.font %]" rel="stylesheet" type="text/css">
[% END -%]
   <!-- Less -->
[% IF config.colours.0 -%]
   <style type="text/less">
   [% FOREACH colour = config.colours -%]
      @[% colour.key %]: [% colour.value %];
   [% END -%]
      @import "[% uri_for( config.less ) %]import/theme-base.less";
   </style>
   <script src="[% uri_for( config.js ) %]less.min.js"></script>
[% ELSE -%]
   <link href="[% uri_for( config.css ) %]theme-[% prefs.theme %].css"
          rel="stylesheet" type="text/css">
[% END -%]
   <!-- Code Highlighter -->
   <script src="[% uri_for( config.js ) %]highlight.min.js"></script>
   <script>hljs.initHighlightingOnLoad();</script>
   <!-- Navigation -->
   <script src="[% uri_for( config.js ) %]jquery.min.js"></script>
   <script src="[% uri_for( config.js ) %]custom.js"></script>
   <title>[% config.title %]</title>
</head>
<body>
   [% content %]
</body>
</html>
